<module>
  <replace-with class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolderDefaultAppearance">
    <when-type-is class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolder.LocalImageHolderAppearance" />
  </replace-with>
  <replace-with class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolderDefaultHighAppearance">
    <when-type-is class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolder.LocalImageHolderAppearance" />
    <any>
      <when-property-is name="mgwt.density" value="high" />
    </any>
  </replace-with>
  <replace-with class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolderDefaultXHighAppearance">
    <when-type-is class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolder.LocalImageHolderAppearance" />
    <any>
      <when-property-is name="mgwt.density" value="xhigh" />
    </any>
  </replace-with>
  <replace-with class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolderDefaultXXHighAppearance">
    <when-type-is class="com.imaginedreal.mgwt.trafficflow.client.widget.image.LocalImageHolder.LocalImageHolderAppearance" />
    <any>
      <when-property-is name="mgwt.density" value="xxhigh" />
    </any>
  </replace-with>
</module>
